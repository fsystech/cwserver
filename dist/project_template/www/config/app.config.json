{
  "$schema": "../../node_modules/cwserver/schema.json",
  "Author": "Safe Online World Ltd.",
  "appName": "cwserver",
  "version": "0.0.1",
  "packageVersion": "1",
  "isDebug": true,
  "template": {
    "cache": true,
    "cacheType": "FILE"
  },
  "encryptionKey": "aaa$a",
  "socketPath": "",
  "staticFile": {
    "fileCache": true,
    "compression": true,
    "minCompressionSize": 0
  },
  "cacheHeader": {
    "maxAge": "7d",
    "serverRevalidate": true
  },
  "liveStream": [],
  "noCache": [],
  "session": {
    "cookie": "_session",
    "maxAge": "1d",
    "key": "adfasf$aa",
    "isSecure": false
  },
  "mimeType": [
    "css",
    "js",
    "png",
    "gif",
    "ico",
    "map",
    "mp4",
    "pdf"
  ],
  "defaultExt": ".html",
  "bundler": {
    "enable": true,
    "fileCache": true,
    "route": "/app/api/bundle/",
    "compress": true
  },
  "views": [ "$root/$public/lib/view/main.js" ],
  "errorPage": {
    "401": "$root/$public/error_page/401.html",
    "404": "$root/$public/error_page/404.html",
    "500": "$root/$public/error_page/500.html"
  },
  "hiddenDirectory": [],
  "hostInfo": {
    "origin": [],
    "root": "www",
    "hostName": "",
    "frameAncestors": "",
    "tls": false,
    "cert": {},
    "port": 8080
  },
  "database": []
}