{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "no-bitwise": false,
    "max-classes-per-file": false,
    "one-variable-per-declaration": false,
    "forin": false,
    "no-namespace": true,
    "radix": false,
    "array-type": [false],
    "no-empty": false,
    "no-trailing-whitespace": false
  },
  "rulesDirectory": []
}